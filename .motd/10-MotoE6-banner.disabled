#!/data/data/com.termux/files/usr/bin/env bash

test $(which figlet) &>/dev/null
if [ $? != 0 ]; then
    echo -e "filget not found.\nPlease install figlet first"
    exit 1
else
    PURPLE='\e[1;95m'
    NC='\033[0m'
    printf "${PURPLE}"
    figlet -S -f lean     "MOTO E6"
    printf "${PURPLE}"
fi 
